<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/fragment_dis_use"
        layout="@layout/topbar"></include>

    <ScrollView
        android:id="@+id/fragment_coupons_use_sv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fragment_dis_use"
        android:layout_marginTop="10dp"
        android:scrollbars="none">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.svgouwu.client.view.MyListView
                android:id="@+id/fragment_coupons_use_Lv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></com.svgouwu.client.view.MyListView>

            <TextView
                android:id="@+id/fragment_coupons_use_tvSub"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/fragment_coupons_use_Lv"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/fragment_dislist_del_bg"
                android:gravity="center"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="确定"
                android:textColor="@color/color_divider"
                android:textSize="12dp" />
        </RelativeLayout>
    </ScrollView>


</RelativeLayout>